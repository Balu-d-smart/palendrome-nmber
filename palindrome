#include <stdio.h>
int main() {
   int n,temp,rev=0,r;
   printf("enter a number:");
   scanf("%d",&n);
   temp=n;
   while(n>0)
   {
   r=n%10;
   rev=(rev*10)+r;
     n=n/10;
    
 }
   if(temp==rev)
       printf("the number is palendrome number");  
else
    printf("the number is  not palendrome number");
    return 0;
}
